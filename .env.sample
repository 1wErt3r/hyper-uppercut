# Required Configuration
HYPER_UPPERCUT_SECRET_KEY=your_nostr_private_key_in_hex_format
HYPER_UPPERCUT_OPML_SOURCE=https://example.com/feeds.opml
HYPER_UPPERCUT_RELAY_URLS=wss://relay.damus.io,wss://relay.nostr.band
